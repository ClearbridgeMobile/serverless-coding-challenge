  "scripts": {
    "pre-commit": "lint-staged",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "concurrently \"npm run db:migrate\" \"npm run start:app\"",
    "start:app": "NODE_ENV=staging nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=prod node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:create-migration": "npm run typeorm -- migration:create",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./typeOrm.config.ts",
    "typeorm:revert-migration": "npm run typeorm migration:revert -- -d ./typeOrm.config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:generate",
    "db:migrate": "TZ=UTC node_modules/.bin/sequelize db:migrate",
    "db:seed": "TZ=UTC node_modules/.bin/sequelize db:seed:all",
    "db:seed:rollback": "TZ=UTC node_modules/.bin/sequelize db:seed:undo:all",
    "db:undo": "TZ=UTC && node_modules/.bin/sequelize db:migrate:undo",
    "db:rollback": "TZ=UTC && node_modules/.bin/sequelize db:migrate:undo:all",
    "console:dev": "ts-node -r tsconfig-paths/register app.console.ts",
    "console": "node dist/app.console.ts"
  },
